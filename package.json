{
  "name": "silva-mongoMart",
  "version": "1.0.0",
  "description": "Express 4.x app ",
  "author": "Silva",
  "license": "Unlicense",
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": "8.11.2"
  },
  "scripts": {
    "start": "SET NODE_ENV=production && node ./bin/app.js",
    "postinstall": "webpack -p --config ./webpack.config.js --progress",
    "build": "SET NODE_ENV=production && webpack -p",
    "silva": "webpack --watch --progress",
    "silva:nowatch": "webpack --progress",
    "deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "deploy-raw": "firebase deploy",
    "test": "jest --colors --env=node --expand --verbose",
    "test:watch": "jest --colors --env=node --expand --verbose --watch"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "babel-plugin-transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.3.5",
    "dotenv": "^5.0.1",
    "ejs": "^2.3.3",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "jest": "^23.1.0",
    "moment": "^2.22.0",
    "mongodb": "^3.0.5",
    "mongoose": "^5.1.4",
    "morgan": "^1.6.1",
    "newrelic": "^3.3.0",
    "qs": "^6.5.2",
    "query-string": "^6.1.0",
    "react": "^16.2.0",
    "react-bootstrap-sweetalert": "^4.4.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.4",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase-tools": "^3.17.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^22.4.4",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.20.2",
    "webpack": "^3.3.0"
  }
}
